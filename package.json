{
  "name": "administrador-charmss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ci": "playwright test --reporter=dot",
    "test:e2e:ci:report": "playwright show-report",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.5",
    "@tailwindcss/postcss": "^4.1.18",
    "axios": "^1.4.0",
    "framer-motion": "^12.23.14",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.5.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^14.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^0.34.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{png,jpg,jpeg,gif,svg}": "git add"
  }
}
